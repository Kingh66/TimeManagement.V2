@model TimeManagement.V2.Models.Module

<h2>Delete Module</h2>

<p>Are you sure you want to delete this module?</p>

<form asp-controller="Module" asp-action="Delete" method="post">
    <input type="hidden" asp-for="ID" />

    <button type="submit">Delete</button>
    <a asp-controller="Module" asp-action="Index">Cancel</a>
</form>

